{
  "manifest_version": 2,

  "name": "Tab Manager",
  "description": "Testing.",
  "version": "1.0",
  "permissions": [
    "<all_urls>",
    "tabs",
    "notifications",
    "activeTab"
    ],

  "web_accessible_resources": [
    "icon.png"
  ],

  "browser_action": { 
    "default_icon": "Media/icon-16.png",
    "default_popup": "popup.html"
  },

  "chrome_url_overrides": {
    "newtab": "visual.html"
  },

  "background": {
    "scripts": ["extension.js", "db.js", "taskdb.js", "visual.js"],
    "persistent": true
  },

  "icons": {
    "16": "Media/green-icon.png", 
    "48": "Media/green-icon.png",
    "128": "Media/green-icon.png"
  },

  "commands": {
    "add-importance-1": {
      "suggested_key": {
        "default": "Ctrl+I",
        "mac": "Command+I"
      },
      "description": "1."
    },
    "add-importance-2": {
      "suggested_key": {
        "default": "Ctrl+K",
        "mac": "Command+K"
      },
      "description": "2."
    },
    "add-importance-3": {
      "suggested_key": {
        "default": "Ctrl+J",
        "mac": "Command+J"
      },
      "description": "3."
    },
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+U",
        "mac": "Command+B"
      }
    }
  }
}
