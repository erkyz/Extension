{
  "manifest_version": 2,

  "name": "Tab Manager",
  "description": "Testing.",
  "version": "1.0",

  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+A",
        "mac": "Command+Shift+A"
      }
    }
  }
}